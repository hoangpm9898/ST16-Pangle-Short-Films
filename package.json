{
	"name": "@rapid/rapid-api-hub",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"test:video": "npx ts-node src/modules/short-film/test-video-upload.ts",
		"prisma:reset": "prisma migrate reset",
		"prisma:generate": "prisma generate --generator client",
		"prisma:generate-dbml": "prisma generate --generator dbml",
		"prisma:seed": "npx tsx prisma/seed.ts"
	},
	"dependencies": {
		"@nestjs/axios": "^4.0.0",
		"@nestjs/bull": "^11.0.3",
		"@nestjs/cache-manager": "^3.0.1",
		"@nestjs/common": "^11.1.0",
		"@nestjs/core": "^11.1.0",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^11.1.0",
		"@nestjs/schedule": "^6.0.0",
		"@nestjs/swagger": "^11.2.0",
		"@nestjs/throttler": "^6.4.0",
		"@prisma/client": "^6.12.0",
		"axios": "^1.9.0",
		"bull": "^4.16.5",
		"cache-manager": "^7.0.1",
		"cache-manager-redis-store": "^3.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"dotenv": "^16.5.0",
		"googleapis": "^154.0.0",
		"passport": "^0.7.0",
		"passport-custom": "^1.1.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"threads-api-wrapper": "^1.0.3",
		"znv": "^0.5.0",
		"zod": "^3.24.4"
	},
	"devDependencies": {
		"@nestjs/cli": "^11.0.7",
		"@nestjs/schematics": "^11.0.5",
		"@nestjs/testing": "^11.1.0",
		"@types/express": "^5.0.1",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.15.18",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"prisma": "^6.12.0",
		"prisma-dbml-generator": "^0.12.0",
		"supertest": "^7.1.1",
		"ts-jest": "^29.3.2",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=9.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
